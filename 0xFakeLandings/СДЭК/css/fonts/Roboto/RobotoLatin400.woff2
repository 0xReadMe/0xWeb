<script>
    var button_general_global = 'button_general';
    var phone_number = '+7(800)222-80-00';
    $(document).ready(function() {
        var tooltip,
            toID,
            appWidth = window.innerWidth,
            telergram_url = 'tg://resolve?domain=Boxberrybot',
            viber_url = 'viber://pa/?chatURI=BoxberryBot',
            button_state = 'active',
            initialStateAnimate = {
                opacity: 1,
                right: "75px",
                width: "200px"
            };
        window.addEventListener("resize", function() {
            appWidth = window.innerWidth
        });

        webim = {
            accountName: "boxberryru",
            domain: "boxberryru.webim.ru",
            location: "boxberryru"
        };

        (function () {
            var s = document.createElement("script");
            s.type = "text/javascript";
            s.src = "https://boxberryru.webim.ru/js/button.js";
            document.getElementsByTagName("head")[0].appendChild(s);
        })();
        function general_close_toggle() {
            var new_state = (button_general_global == 'button_general' ? 'button_ico_close' : 'button_general');
            $('.messenger-'+ button_general_global).addClass('messenger-'+ new_state);
            $('.messenger-'+ new_state).removeClass('messenger-'+ button_general_global);
            button_general_global = new_state;
        }
        function close_tooltip(element){
            $(element).find(".main-content").show();
            $(element).find(".thinks-too-long").hide();
            if( !$(element).parent().hasClass('messenger-button_ico_livetex') ||
                !$(element).parent().hasClass('messenger-button_ico_close') ) {
                $(element).width(initialStateAnimate.width+'px');
                $(element).height('40px');
            }
            element.animate({
                opacity: 0,
                right: "0px",
            },  function(){
                $(this).css({width: "0px"});
            });
            if($(element).parent().offset()){
                $(element).offset({top:$(element).parent().offset().top});
            }
        }
        function tooltip_animate (element, fadeIn, fadeOut, delay) {
            var callbackAnimate = function() {
                if (delay > 0) {
                    $(element).delay(delay).animate({
                        opacity: 0,
                        right: "0px",
                    }, fadeOut, function(){
                        $(this).css({width: "0px"});
                    });
                }
            };

            element.clearQueue();
            element.animate(initialStateAnimate, fadeIn,  callbackAnimate);
        }
        function toggle_button_state() {
            general_close_toggle();
            previous_button_state = button_state;
            button_state = (button_state == 'unactive') ? 'active' : 'unactive';
            $('.messenger-button_' + button_state ).each(function(){
                $(this).removeClass('messenger-button_' + button_state);
                $(this).addClass('messenger-button_' + previous_button_state);
            });
        }

        $('.messenger-button_ico_close').on('click', function(){
            toggle_button_state();
        });

        $('.messenger-button_ico_livetex').on('click', function(){
            toggle_button_state();
        });

        $(".messenger-button_ico_telegram").on('click', function () {
            //toggle_button_state();
            if (appWidth > 720) {
                toID = setTimeout(function(){ showQRPopUp(this) }.bind(this), 2000);
            }
            window.open(telergram_url,window.name);
            return false;
        });

        $(".messenger-button_ico_viber").on('click', function (e) {
            //toggle_button_state();
            if (appWidth > 720) {
                toID = setTimeout(function(){ showQRPopUp(this) }.bind(this), 2000);
            }
            window.open(viber_url,window.name);
            return false;
        });

        $('.messenger-button_ico_phone').on('click', function () {
            toggle_button_state();
            window.open("tel:" + phone_number);
        });

        $('.messenger-button_general').on('click', function () {
            var delayTime = (appWidth > 720 ? 1500 : 0);
            toggle_button_state();
            if ($(this).hasClass('messenger-button_ico_close')) {
                //return; //отключаем одновременное всплытие блоков с подсказками для кнопок мессенжеров и чата
                if ( appWidth <= 720 ){
                    tooltip_animate($('.messenger-tooltip'), 500, 1000, delayTime);
                }else{
                    $('.messenger-tooltip').css(initialStateAnimate)
                    $('.messenger-tooltip').delay(delayTime).animate({
                        opacity: 0,
                        right: "0px",
                    }, 1000, function() {
                        $(this).css({width: "0px"});
                    });
                }
            } else {
                if (appWidth <= 720) {
                    close_tooltip($('.messenger-tooltip'));
                }
            }
        });

        $('.messenger-button').on("hover",
            function(e) {
                if(e.type == "mouseenter") {
                    var $this = $(this);
                    if (appWidth > 720) {
                        tooltip = $(this).children('.messenger-tooltip');
                        $.each(
                            $(".messenger-button"),
                            function (key, val) {
                                if (val !== $this.get(0)) {
                                    close_tooltip($(val).find(".messenger-tooltip"));
                                }
                            }
                        );
                        if (tooltip.length > 0 &&
                            $this.find(".messenger-tooltip").css("right") === "0px") {
                            tooltip_animate(tooltip, 500, 1000, 0);
                        }
                    }
                } else if (e.type == "mouseleave") {
                    //clearTimeout(toID);
                }
            }
        );

        function showQRPopUp(obj) {
            if ( $(obj).hasClass('messenger-button_active') ) {
                $(obj).find('.main-content').hide();
                var height = $('.popup-messenger').height();
                $(obj).find('.messenger-tooltip').animate(
                    {
                        width:'313px',
                        height: ((height + 120) + 'px'),
                        opacity: initialStateAnimate.opacity,
                        right: initialStateAnimate.right
                    }
                );
                $(obj).find('.thinks-too-long').fadeIn(1000);
                var offsetTop = $('.messenger-button_ico_livetex .messenger-tooltip').offset().top - 130;
                $(obj).find('.messenger-tooltip').offset({top:offsetTop});
                $(obj).find('.notch-right').offset({
                    top:$(obj).offset().top + 15,
                    left:$(obj).offset().left - 17
                });
            }
        }

        $(window).resize(
            function() { //перерисовываем стрелку на кнопку мессенджера при ресайзе окна
                $.each(
                    $('.messenger-button_active'),
                    function (key,val) {
                        if ($(val).find(".messenger-tooltip").css("right") !== "0px") {
                            $(val).find('.notch-right').offset({
                                top: $(val).offset().top + 15,
                                left: $(val).offset().left - 17
                            });
                        }
                    }
                );
            }
        );
    })
